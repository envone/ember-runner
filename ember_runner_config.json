{
  
  "apps": { 
    "input": "apps",
    "styles": "css",
    "static": "static",
    "templates": "templates",
    "scripts": "js",
    "output": "public/apps"
  },
  
  "vendors": {
    "input": "vendors",
    "distributions": {
      "ember.js": ["ember.js/packages/handlebars", "ember.js/packages/ember-metal", "ember.js/packages/ember-runtime", "ember.js/packages/ember-views", "ember.js/packages/ember-states", "ember.js/packages/metamorph", "ember.js/packages/ember-handlebars"],
      "addons.js": ["sproutcore-datastore", "sproutcore-routing", "sproutcore-statechart", "sproutcore-touch"],
      "shared.js": ["envone-core", "envone-desktop-iu", "envone-desktop", "business-core"]
    },
    "styles": "css",
    "static": "static",
    "templates": "templates",
    "scripts": "lib",
    "output": "public/shared"
  },
  
  "tmpDir": "tmp",
  "distDir": "dist",
  
  "server": {
    "static": "public",
    "port": "3000",
    "proxy": {
      "useProxy": true,
      "host": "localhost",
      "port": 3101,
      "prefix": "^\/backend"
    }
  }
  
}